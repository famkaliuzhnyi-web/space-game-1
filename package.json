{
  "name": "space-game-1-godot",
  "private": true,
  "version": "2.0.0",
  "description": "Space Game 1 - Godot Engine Version",
  "type": "module",
  "scripts": {
    "dev:orig": "cd orig && npm run dev",
    "build:orig": "cd orig && npm run build",
    "test:orig": "cd orig && npm run test",
    "install:orig": "cd orig && npm install",
    "godot:export": "godot --headless --export-release Web ./build/",
    "godot:run": "godot --main-pack project.godot",
    "setup": "npm run install:orig && echo 'Original dependencies installed. Open project.godot in Godot Editor for engine development.'"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "devDependencies": {
    "@types/node": "^24.2.0"
  }
}